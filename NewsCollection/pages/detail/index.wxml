<View class="container">
<view class="title"> {{article.title}}</view>
<view class="poster">
  <image src="{{article.poster}}"></image>
</view>

<view class="center">
  <text>  {{article.center}} </text>
</view>
<view class="date"> {{article.date}}</view>
</View>
<button wx:if="{{isAdd}}" bindtap="isCel" plain> 已收藏</button>
<button wx:else plain bindtap="isAdds"> 未收藏</button>